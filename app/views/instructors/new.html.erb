<%= form_with model: @instructor do |form| %>
<%= form.label :username %><br>
 <%= form.text_field :username %><br>
 <%= form.label :first_name %><br>
 <%= form.text_field :first_name %><br>
 <%= form.label :last_name %><br>
 <%= form.text_field :last_name %><br>
 <%= form.label :encrypted_password %><br>
 <%= form.password_field :encrypted_password %><br>
 <%= form.label :age %><br>
 <%= form.number_field :age, min: 0, max:150 %><br>
 <%= form.label :salary %><br>
 <%= form.number_field :salary, min:0 %><br>
 <%= form.label :highest_completed_education %><br>
 <%= form.select :highest_completed_education, options_for_select([['High School'], ['Bachelors'], ['MBA'], ['PHD']]) %><br>
 <%= form.submit %>
<% end %>