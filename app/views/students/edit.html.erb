<%= form_with model: @student, url: "/students/new" do |form| %>
  Firstname: <br> <%= form.text_field :first_name %><br>
  Lastname: <br> <%= form.text_field :last_name %><br>
  Username: <br> <%= form.text_field :username %><br>
  <%= form.label :birthdate %><br>
  <%= form.date_field :birth_date %><br>
  <%= form.label :password %>
  <br> <%= form.password_field :password %><br>
  <%= form.label :highest_completed_education %><br>
 <br> <%= form.select :highest_completed_education, options_for_select(['High School','Bachelors','MBA','PHD']) %><br>
  <%= form.submit %>
<% end %>
