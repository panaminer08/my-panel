<%= link_to "Create Student", new_student_path %>

<% @student.each do |s| %>
    <div id="delete<%= s.id %>">
    <div class="container">
    <table style= "width: 100%">
        <%=link_to "Edit", edit_student_path(s.id) %><br>
        <%= link_to "Delete", student_path(s.id), method: :delete, remote: true %>
    <tr>
        <th>Student ID</th>
        <th>Frist Name</th>
        <th>Last Name</th>
        <th>Username</th>
        <th>Age</th>
        <th>Highest Education Achieved</th>
    </tr>
    <tr>
        <td><%= s.id %></td>
        <td> <%= s.first_name %></td>
        <td><%= s.last_name %></td>
        <td><%= s.username %></td>
        <td><%= s.age %></td>
        <td><%= s.highest_completed_education %></td>
    </tr>

</table>
</div>
</div>
<% end %>
